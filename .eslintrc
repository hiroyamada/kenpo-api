{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "never"],
    "strict": [2, "never"],
    "no-param-reassign": [0, { "props": false }],
    "no-console": 0,
    "consistent-return": 0
  },
  "extends": "airbnb"
}
